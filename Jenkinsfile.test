pipeline {
  agent { label 'docker' }

  environment {
    GIT_EMAIL="${GIT_EMAIL}"
    GIT_USERNAME="${GIT_USERNAME}"
    GIT_REMOTE_URL="${GIT_REMOTE_URL}"
    GIT_REMOTE_NAME="${GIT_REMOTE_NAME}"
  }

  stages {
    stage('Test') {
      steps {
        sh './docker/test'
      }
    }
  }
  post {
    always {
       echo 'Post post action ran.'
       echo "${payload}"
       echo '...'
    }
    success {
       echo 'Success post action ran.'
    }
    failure {
       echo 'Failure post action ran.'
    }
  }
}
